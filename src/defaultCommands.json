{
    "nv": "node -v",
    "n": "node",
    "ni": "npm i",
    "nr": "npm run {{arg}}",
    "nls": "npm ls",
    "nun": "npm uninstall {{arg}}",
    "ns": "npm start",
    "nt": "npm test",

    "gs": "git status",
    "ga": "git add {{arg}}",
    "gaa": "git add .",
    "gc": "git commit -m \"{{arg}}\"",
    "gca": "git commit --amend",
    "gp": "git push",
    "gpf": "git push --force",
    "gpu": "git push -u origin {{arg}}",
    "gl": "git pull",
    "gco": "git checkout {{arg}}",
    "gcob": "git checkout -b {{arg}}",
    "gb": "git branch",
    "gba": "git branch -a",
    "gbd": "git branch -d {{arg}}",
    "gm": "git merge {{arg}}",
    "gd": "git diff",
    "gds": "git diff --staged",
    "grs": "git reset",
    "grh": "git reset --hard",
    "gcl": "git clone {{arg}}",
    "glog": "git log --oneline",
    "gsh": "git show",

    "yi": "yarn install",
    "ya": "yarn add {{arg}}",
    "yad": "yarn add -D {{arg}}",
    "yag": "yarn global add {{arg}}",
    "yr": "yarn run {{arg}}",
    "yt": "yarn test",
    "yrm": "yarn remove {{arg}}",
    "yup": "yarn upgrade {{arg}}",
    "yls": "yarn list --depth=0"
}